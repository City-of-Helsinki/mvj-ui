{
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["module-alias", [
      { "src": "./src", "expose": "$src" },
      { "src": "./assets", "expose": "$assets" },
      { "src": "./src/components", "expose": "$components" },
      { "src": "./src/util", "expose": "$util" },
    ]]
  ]
}
