[include]
.*/src/

[ignore]
.*/node_modules/fbjs
.*/node_modules/editions/source
.*/node_modules/react-leaflet
.*/node_modules/react-select

[libs]
./defs
./flow-typed

[options]
module.name_mapper.extension='css' -> 'CSSModule'
module.name_mapper.extension='svg' -> 'EmptyModule'
module.name_mapper='^\$assets/\(.*\)$' -> '<PROJECT_ROOT>/assets/\1'
module.name_mapper='^\$components/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^\$src/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^\$util/\(.*\)$' -> '<PROJECT_ROOT>/src/util/\1'

# suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
